{
  "source": {
    "include": ["./src/"],
    "excludePattern": ".__mocks__."
  },
  "sourceType": "module",
  "opts": {
    "destination": "./dist/doc",
    "readme": "./jsdoc/HOME.md",
    "encoding": "utf8",
    "template": "./node_modules/docdash",
    "recurse": true,
    "verbose": true
  },
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "systemName": "",
    "logoFile": "",
    "theme": "darkly",
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "footer": "",
    "copyright": "",
    "navType": "vertical",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "protocol": "html://",
    "methodHeadingReturns": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
