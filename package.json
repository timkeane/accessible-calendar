{
  "name": "accessible-event-calendar",
  "version": "1.2.1",
  "description": "Accessible event calendar backed by CSV data",
  "homepage": "https://github.com/timkeane/accessible-event-calendar",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "webpack --config ./webpack.mini.config.js",
    "postbuild": "node release.js",
    "prerelease": "npm run clean",
    "release": "webpack --config ./webpack.mini.config.js && webpack --config ./webpack.unmini.config.js",
    "postrelease": "node release.js",
    "prepkg": "npm run clean && mkdir dist",
    "pkg": "cp ./package.json ./dist/ && cp ./src/js/CsvEventCalendar.js ./dist/index.js  && cp ./src/css/* ./dist/",
    "postpkg": "cd dist && npm pack"
  },
  "author": "Tim Keane <t.d.keane@gmail.com> (https://timkeane.github.io/)",
  "license": "Apache-2.0",
  "dependencies": {
    "countries-and-timezones": "^3.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "jquery": "^3.6.2",
    "papaparse": "^5.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "copy-webpack-plugin": "^10.2.0",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "jest": "^29.3.1",
    "mini-css-extract-plugin": "^2.4.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.10.0",
    "zip-a-folder": "^1.1.5"
  },
  "jest": {
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/js/*.js"
    ]
  }
}
